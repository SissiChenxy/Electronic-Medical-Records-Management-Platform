<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Root Context: defines shared resources visible to all other web components -->
	<bean name="userDao" class="com.me.EMR.dao.UserDAO" scope="request"></bean>
	<bean name="messageDao" class="com.me.EMR.dao.MessageDAO" scope="request"></bean>
	<bean name="appointmentDao" class="com.me.EMR.dao.AppointmentDAO" scope="request"></bean>
	<bean name="recipeDao" class="com.me.EMR.dao.RecipeDAO" scope="request"></bean>
	
</beans>
